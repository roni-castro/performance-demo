{
  "name": "performance-demo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:release": "npx expo start --no-dev --minify",
    "reset-project": "node ./scripts/reset-project.js",
    "dev": "APP_VARIANT=development npx expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "open-atlas": "sh -c \"if command -v open >/dev/null 2>&1; then open 'http://localhost:8081/_expo/atlas'; else xdg-open 'http://localhost:8081/_expo/atlas'; fi\" && EXPO_UNSTABLE_ATLAS=true npm run start:release",
    "test": "jest --watchAll",
    "test:e2e": "maestro test e2e",
    "lint": "expo lint",
    "android:build": "eas build --platform android",
    "android:submit": "eas submit --platform android",
    "unused-deps": "npx depcheck"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@lottiefiles/dotlottie-react": "^0.9.2",
    "@react-navigation/native": "^6.0.2",
    "@shopify/flash-list": "1.7.1",
    "expo": "^52.0.0-preview.10",
    "expo-constants": "~17.0.2",
    "expo-dev-client": "~5.0.0-preview.3",
    "expo-font": "~13.0.0",
    "expo-linking": "~7.0.2",
    "expo-router": "~4.0.0-preview.6",
    "expo-splash-screen": "~0.28.4",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.1",
    "expo-updates": "~0.26.2",
    "lottie-react-native": "7.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.0.0-beta.14",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.0.7",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-native": "^4.1.0",
    "expo-atlas": "^0.3.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.0-preview.2",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "overrides": {
    "react-native-screens": "4.0.0-beta.14"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@lottiefiles/dotlottie-react"
        ]
      }
    }
  },
  "private": true
}
